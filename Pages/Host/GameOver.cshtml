          <p id="coupleTwo">
            @ViewData["CoupleTwoName"]:
            <label class="votecard-cc @ViewData["q2c2vote"]" for="@ViewData["q2c2vote"]" />
          </p>
        </div>
        <div class="col-md-4 resultThree">
          <h3 id="question">Q3: @ViewData["q3Title"]</h3>
          <h5 id="audience">The Audience said...</h5>


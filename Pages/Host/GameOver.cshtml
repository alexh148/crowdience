@page
@model HostGameOverModel
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <h3 id="gameOver">GameOver :(</h3>
      <h3 id="stat">Stat Time!</h3>
      <div class="row">
        <div class="col-md-4 resultOne">
          <h3 id="question">Q1: @ViewData["q1Title"]</h3>
          <h5 id="audience">The Audience said...</h5>
          <p id="q1a1Icon">
            <p id="a1Container">
              <span id="q1a1">@ViewData["q1a1"]</span>
            </p>
          </p>
          <p id="q1a2Icon">
            <p id="a2Container">
              <span id="q1a2">@ViewData["q1a2"]</span>
            </p>
          </p>
          <canvas id="bc1"></canvas>
          <h5 id="couple">The Couple said...</h5>
          <p id="coupleOne">
            @ViewData["CoupleOneName"]:
            <label class="votecard-cc @ViewData["q1c1vote"]" for="@ViewData["q1c1vote"]" />
          </p>
          <p id="coupleTwo">
            @ViewData["CoupleTwoName"]:
            <label class="votecard-cc @ViewData["q1c2vote"]" for="@ViewData["q1c2vote"]" />
          </p>
        </div>
        <div class="col-md-4 resultTwo">
          <h3 id="question">Q2: @ViewData["q2Title"]</h3>

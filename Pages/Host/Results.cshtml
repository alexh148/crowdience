@page
<body>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-9">
      <div class="row">
        <div class="col-md-9">
          <h1 id="questionTitle">
            Question Title!
          </h1>
        </div>
        <div class="col-md-3">
          <div id="buttonDiv">
            <button id="nextQuestion">Next Question!</button>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-3">
          <p id="answerOne">Answer One!:</p>
        </div>
        <div class="col-md-3">
          <p id="answerOneCounterP"><span id="answerOneCounter">0</span></p>
        </div>
        <div class="col-md-3">
          <p id="answerTwo">Answer Two:</p>
        </div>
        <div class="col-md-3">
          <p id="answerTwoCounterP"><span id="answerTwoCounter">0</span></p>
        </div>
        
      </div>
      <div class="row">
        <div class="col-md-12">
          <canvas id="bar-chart-horizontal">
				</div>
			</div>
		</div>
		<div class="col-md-3">
      <h3 id="usersJoined">User Votes:</h3>
      <ul id="messagesList">
      </ul>
		</div>
	</div>
</div>
</body>
